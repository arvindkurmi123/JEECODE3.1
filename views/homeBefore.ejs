<style>
.cards-container {
  display: flex;
  grid-template-columns: repeat(3, 1fr); /* 3 cards in each row */
  gap: 10px;
  padding: 20px;
  justify-content: center;
  margin: 0 auto;
  max-width: 100%;
  
}

.card {
  background-color: #fff;
  color: #333;
  border-radius: 10px;
  overflow: hidden;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.card-content {
  padding: 20px;
}

.card-title {
  font-size: 24px;
  margin: 0;
}

.card-info {
  font-size: 16px;
  margin-top: 10px;
}

.card-details {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20px;
  font-size: 14px;
}

.progress-bar {
  width: 80%;
  background-color: #eee;
  height: 10px;
  border-radius: 5px;
  overflow: hidden;
}

.progress {
  width: 50%; /* Adjust the width as needed */
  height: 100%;
  background-color: #3498db; /* Progress bar color */
}

.course-cost {
  font-weight: bold;
  color: #e74c3c; /* Cost color */
}

.enrollment-info {
  color: #555;
}

.chapter-count {
  font-size: 14px;
  margin-top: 5px;
}

.play-button {
  background-color: #ffffff;
  color: #000000aa;
  border: none;
  border-radius: 8px;
  padding: 5px 10px;
  cursor: pointer;
  font-size: 17px;
  margin-top: 20px;
  box-shadow: 0 4px 6px rgba(255, 255, 255, 0.4);
  font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
}

.play-button:hover {
  background-color: #ffffff;
  color: black;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.6);

}

/* Media Queries for Responsiveness */
@media screen and (max-width: 768px) {
  .cards-container {
      grid-template-columns: 1fr; /* Display a single column on smaller screens */
  }

  .card {
      width: 100%;
      margin: 0 auto 20px;
  }

  .card-content {
      padding: 10px;
  }

  .card-title {
      font-size: 60px;
      color: black;

  }

  .card-info {
      font-size: 14px;
  }

  .card-details {
      font-size: 12px;
  }

  .progress-bar {
      width: 80%;
  }
 
}
</style>
